<body class=" justify-content-center " >

<section class="vizew-post-area mb-50">
  
   
    <div class="container text-justify mb-4">
 
    <div class="row">
        <div class="col-md-12 col-md-offset-12">
     
    <div class="all-posts-area ">
   <div id="">
        
      <form  class="form-signin" action="/reporters" id="reporter" method="POST">
           
        <div >    <%- messages('message', locals) %></div>  

        <div class="form-row">
          <div class="form-group col-md-6">
              <label for="name" class="textlabel" >Name</label>
            <input required type="text" class="form-control textboxes" id="name" name = "name" title="enter Name" placeholder="e.g John">
           
          </div>
          <div class="form-group col-md-6">
                <label for="surname" class="textlabel" >Surname</label>
              <input required type="text" class="form-control textboxes" id="surname" name = "surname" title="enter Surname" placeholder="e.g Doe">
             
            </div>

            <div class="form-group col-md-6">
       
                <label for="facebook" class="textlabel" >Facebook</label>
                <input required type="text" class="form-control textboxes" id="facebook" name = "facebook" title="enter Facebook handle" placeholder="e.g John Doe">
                            
            </div>
            <div class="form-group col-md-6">
       
                        <label for="instagram" class="textlabel" >Instagram</label>
                      <input  type="text" class="form-control textboxes" id="instagram" 
                      name = "instagram" title="enter Instagram handle" placeholder="e.g @John" pattern="^@[A-Za-z0-9_]{1,15}$">
                     
                    </div>
            <div class="form-group col-md-6">
       
                    <label for="twitter" class="textlabel" >Twitter</label>
                    <input  type="text" class="form-control textboxes" id="twitter" 
                    name = "twitter" title="enter Twitter handle" placeholder="e.g @John" pattern="^@[A-Za-z0-9_]{1,15}$">
                    
                </div>
                <div class="form-group col-md-6">
                        <label for="phone" class="textlabel" >Phone No.</label>
                        <input required type="tel" class="form-control textboxes" id="phone" name = "phone" 
                        pattern="\\+{0,1}[0-9]{10,12}" title="enter your phone number" placeholder="e.g +2345006078">
                        
                    </div>

        </div>
           
        <div class="form-group">
       
                <label for="email" class="textlabel">Email address</label>
                <input type="email" class="form-control textboxes" name="email" id="email" aria-describedby="emailHelp" placeholder="e.g. doe@email.com ensure email is valid" required>
                <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
              </div>
        <div class="form-row">
            <label for="profile" class="textlabel">Profile</label>
            <textarea required name="profile" class="form-control textboxes" id="profile" cols="30" rows="10"
             title="Give a brief history of your reporting career" placeholder="Give a brief history of your reporting career"></textarea>
            
        </div>

        <div class="form-row button-wrapper">
            <div class="col-12 col-md-6">
                <button type="submit" class="btn btn-primary btn-danger btn-block">Submit</button>
       
            </div>
            <div class="col-12 col-md-6">
          <button type="submit" class="btn btn-primary btn-danger btn-block" formaction="/exit">Exit</button>
          </div>
          </div>
          
        </form>
         
    </div>
    </div>
    </div>
    </div>
    </div>
          
