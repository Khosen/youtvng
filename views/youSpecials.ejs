        <form action="" id="commentForm" method="">
              <div class="form-group">
                 <% for(var x=0; x<docs.length; x++) {%>
     
                                      
                                   
                 <input name="postId" type="hidden" id ="postId" value="<%= docs[x].postId %>"> </br>
                                     
                 <% } %>

                <label for="comments"><h6>Leave a Comment</h6></label>
                <p>  </p>

                <input type="text" id="name"  name="name"class="form-control editboxes" placeholder="Name">
                            
                <textarea class="form-control editboxes" name="comment" id="comment" rows="3" placeholder="Comments"></textarea>
              </div>
              <button class="btn vizew-btn mt-30" id="send"  value="Send" type="submit">Submit Comment</button>
         
