                              comments.forEach( function ( commentData ){
                        %>
                    <div>
                  
                    <span><%= commentData.name %> &nbsp;</span>
                    <span><%= commentData.date %>  &nbsp;</span>
                    <span><%= commentData.time %> &nbsp; </span>
                      <p> <%= commentData.comment %> </p>
                    </div>
                   
              
                      <% });
                        } %>
