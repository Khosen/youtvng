<section class="post-details-area mb-80">
<div class="container">
<div class="row" >
<div class="col-12">
    <% for(var x=0; x<docs.length; x++) {%>
     
          
<!--<div class="post-details-thumb mb-50">
      
<img src="" style="width:40%; height:70%;"alt="">
</div>-->
   
<div class="post-details-thumb  mb-50" style="width:40%; height:70%;">
 
<div id="carousel-thumb" alt=""class="carousel slide carousel-fade carousel-thumbnails" data-ride="carousel">
  <!--Slides-->
  <div class="carousel-inner" role="listbox" >
    <div class="carousel-item active">
      <img class="d-block w-100" src="/<%= docs[x].photo %>" alt="First slide">
    </div>
    <div class="carousel-item" >
      <img class="d-block w-100" src="/<%= docs[x].photo1 %>"  alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="/<%= docs[x].photo2 %>" alt="Third slide">
    </div>
  </div>
  <!--/.Slides-->
  <!--Controls-->
  <a class="carousel-control-prev" href="#carousel-thumb" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carousel-thumb" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
  <!--/.Controls-->
  <ol class="carousel-indicators" >
    <li data-target="#carousel-thumb"   data-slide-to="0" class="active"><img class="d-block w-100" src="/<%= docs[x].photo %>" class="img-fluid"></li>
    <li data-target="#carousel-thumb"  data-slide-to="1"><img class="d-block w-100" src="/<%= docs[x].photo1 %>"   class="img-fluid"></li>
    <li data-target="#carousel-thumb"  data-slide-to="2"><img class="d-block w-100" src="/<%= docs[x].photo2 %>"  class="img-fluid"></li>
  </ol>

</div>
</div>
</div>
</div>

<div class="row justify-content-center">

<div class="col-12 col-lg-8 col-xl-7">
<div class="post-details-content">

<div class="blog-content">

<div class="post-content mt-0">
   
<a href="#" class="post-cata cata-sm cata-danger"><%= docs[x].subcategory %></a>
<a href="" class="post-title mb-2"><%= docs[x].topic %></a>
<div class="d-flex justify-content-between mb-30">
<div class="post-meta d-flex align-items-center">
<a href="#" class="post-author"><%= docs[x].author %></a>
<i class="fa fa-circle" aria-hidden="true"></i>
<a href="#" class="post-date"><%= docs[x].date %></a>

</div>

<div class="post-meta d-flex post-content">
    
<a href="#"><i class="fa fa-comments-o" aria-hidden="true" id="counts">  <%= docs[x].commentCounts %></i>
</a>
<a href="#"><i class="fa fa-eye" aria-hidden="true"></i> <%= docs[x].viewCounts %></a>
<a href="#"><i class="fa fa-thumbs-o-up" id="clicks" aria-hidden="true">  <%= docs[x].clicks %></i></a>

<!---get click count from db an dplace invisbly-->
                  
<input name="clickvalue" type="hidden" id ="clickvalue" value="<%= docs[x].postId %>" >
       
<% } %>  

</div>
</div>
</div>

<% for(var x=0; x<docs.length; x++) {%>
  
<p> <%= docs[x].news %></p>

<% } %>


