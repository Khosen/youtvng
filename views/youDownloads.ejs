<body>


<header class="header-area">
    
  <% include ./partials/header %>

</header>


<div class="vizew-breadcrumb">
<div class="container">
<div class="row">
<div class="col-12">
<nav aria-label="breadcrumb">
<ol class="breadcrumb">
<li class="breadcrumb-item"><a href="#"><i class="fa fa-home" aria-hidden="true"></i> Home</a></li>
<li class="breadcrumb-item"><a href="#">Feature</a></li>
<li class="breadcrumb-item active" aria-current="page"> AfroMusic</li>
</ol>
</nav>
</div>
</div>
</div>
</div>


<div class="vizew-archive-list-posts-area mb-80">
<div class="container">
<div class="row justify-content-center">
<div class="col-12 col-lg-8">

<div class="archive-catagory-view mb-50 d-flex align-items-center justify-content-between">

<div class="archive-catagory">
<h4><i class="fa fa-users" aria-hidden="true"></i>    AfroMusic </h4>
</div>

</div>
<% for(var x=0; x<docs.length; x++) {%>
 
 
<div class="single-post-area style-2">
<div class="row align-items-center">
<div class="col-12 col-md-6">

<div class="post-thumbnail">

        <img style="height:200px; width:100%"  src="/<%=docs[x].albumArt %>"/>
 </div>
</div>
<div class="col-12 col-md-6">

<div class="post-content mt-0">
<a href="#" class="post-cata cata-sm cata-success"><%= docs[x].title %></a>
<a href="" class="post-title mb-2"><%= docs[x].genre %></a>
<div class="post-meta d-flex align-items-center mb-2">
<a href="#" class="post-author"><%= docs[x].artist%></a>
<i class="fa fa-circle" aria-hidden="true"></i>
<a href="#" class="post-date"><%= docs[x].date %></a>
</div>
<p class="mb-2"><%= docs[x].news %></p>
<div class="post-meta d-flex">
        <a href="/music/downloads/<%=docs[x].slug%>" class="post-cata cata-sm cata-primary">Download song</a>

       <!-- <a href="/news/<%= docs[x].slug%>"><i class="fa fa-comments-o" aria-hidden="true" id="counts">  <%= docs[x].commentCounts %></i>
        </a>
        <a href="#"><i class="fa fa-eye" aria-hidden="true"></i> <%= docs[x].viewCounts %></a>
<a href="#"><i class="fa fa-thumbs-o-up" id="clicks" aria-hidden="true">  <%= docs[x].clicks %></i></a>
--></div>
</div>
</div>
</div>
</div>

<% } %>


