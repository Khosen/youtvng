
<!doctype html>
<html lang="en">


  <%- include('./partials/dashboard') %> 
  
  
  <body>
  
    <%- include('./partials/loginNav') %>

<section id="breadcrumb">
   
<div class="">
  <ol class="breadcrumb">
    <li> <h4>
        <span class="iconify" data-icon="bi-person-fill" data-inline="false" data-height="25">
          </span>
        
        <small>Account Login</small>
      </h4></li>  
    
  </ol>
</div>
</section>

<section id="main">
  <div class="container ">
    <div class= "row align-items-center justify-content-center">

      <div class="col-md-4 ">
        <div class="card ">
          <div class="card-header main-color-bg">
            <span class="iconify" data-icon="door-open-fill" data-inline="false" data-height="25">
            </span>
            Login
          </div>
          <!--website users-->
          <div class="card-body">
            <div class="row ">
              <div class="col">
                <form class="form-signin " method="POST" action="/users/adminConsole">
                   
                  <%- include('./partials/messages') %> 
                
                    <h1 class="h3 mb-3 font-weight-bold " style="color:rgb(136, 19, 19)">Please sign in</h1>
                    
                     
                    <div class="form-group">
                        <input type="email" id="email"
                         class="form-control" name="email" placeholder="nam@eg.com" required autofocus>
                     <!--<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
                     -->
                    </div>
                    <div class="form-group">
                     <!-- <label for="password">Password</label>-->
                      <input type="password" id="password" name="password" class="form-control" data-placement="after" data-toggle="password" maxlength="10" placeholder="Password" required>
                   
                    </div>
                    
                    <span><button type="submit" class="btn btn-danger">Sign in</button>
                      <span> Not Registered? <a href="/users/registerConsole" style="color:#db4437;">Register</a></span>
                      </span>
                   
                  </form>
                  
                 </div>
                    
            </div>
         </div>
        </div>
        </div>
    </div>
   </div>
   
    <br>
           
</section>

     
<footer class="offset-4">
    <div class="footer">
    <p class="mt-1"><b>YouTV &copy; <script type="text/javascript">
        document.write(new Date().getFullYear());
      </script></b></p>
    </div>
</footer>
<%- include('./partials/dashScripts') %>

</body>
</html>
